{
    "manifest_version": 2,
    "name": "ProblemGen",
    "version": "1.0",
  
    "description": "Generate math problems in order with a specific index.",
    "homepage_url": "https://ntdi.world",
    "icons": {
      "48": "icons/icon-48.png"
    },
  
    "permissions": [
      "activeTab"
    ],
  
    "browser_action": {
      "default_icon": "icons/icon-32.png",
      "default_title": "ProblemGen",
      "default_popup": "popup/generate.html"
    }
  }
  